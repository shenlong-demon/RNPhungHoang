{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./" /* Base directory to resolve non-absolute module names. */,
   "paths": {
      "@core/*": ["./core/*"],
      "@src/*": ["./src/*"]
    }
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js",
    ".eslintrc.js",
    ".prettirerrc.js",
    "app"
  ]
}
